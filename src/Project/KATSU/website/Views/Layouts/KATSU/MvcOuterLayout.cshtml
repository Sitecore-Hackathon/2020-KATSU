@using Sitecore.Mvc

@using Sitecore.ExperienceForms.Mvc.Html

@using Sitecore.Mvc.Analytics.Extensions

@{

    Layout = null;

}

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@Html.Sitecore().Field("title", new { DisableWebEdit = true })</title>

    @Html.Sitecore().VisitorIdentification()


    <!-- Global stylesheets -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">
    <link href="~/Content/icons/icomoon/styles.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/bootstrap_limitless.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/layout.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/components.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/colors.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/styles.css" rel="stylesheet" type="text/css">
    <!-- /global stylesheets -->
    <!-- Core JS files -->
    <script src="~/Scripts/main/jquery.min.js"></script>
    <script src="~/Scripts/main/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/plugins/loaders/blockui.min.js"></script>
    <script src="~/Scripts/plugins/ui/slinky.min.js"></script>
    <script src="~/Scripts/plugins/ui/ripple.min.js"></script>
    <!-- /core JS files -->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @Html.RenderFormStyles()

    @Html.RenderFormScripts()

</head>

<body>
    @RenderBody()
</body>
</html>